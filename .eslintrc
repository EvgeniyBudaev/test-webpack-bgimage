{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "quotes": "off",
    "require-jsdoc": "off",
    "no-trailing-spaces": "off",
    "max-len": "off",
    "operator-linebreak": "off"
  },
  "extends": ["eslint:recommended"]
}
